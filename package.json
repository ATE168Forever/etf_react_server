{
  "name": "etf-life-monorepo",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.11.0",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel --filter dividend-life --filter balance-life --filter health-life --filter wealth-life run dev",
    "dev:dividend-life": "pnpm --filter dividend-life dev",
    "dev:budget-life": "pnpm --filter budget-life dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm --filter dividend-life test"
  }
}
